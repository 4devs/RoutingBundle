{
    "name": "fdevs/routing-bundle",
    "type": "symfony-bundle",
    "description": "4devs Routing Bundle",
    "keywords": ["Routing management for mongodb"],
    "homepage": "https://github.com/4devs/routing-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Samusev",
            "email": "andrey@4devs.org",
            "homepage": "http://andrey.4devs.io"
        },
        {
            "name": "4devs Community",
            "homepage": "https://github.com/4devs/routing-bundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.4",
        "cocur/slugify": "~1.0",
        "symfony-cmf/routing-bundle": "~1.3"
    },
    "require-dev": {
        "doctrine/mongodb-odm-bundle": "~3.0",
        "doctrine/mongodb-odm": "1.0.*@dev"
    },
    "suggest": {
        "sonata-project/admin-bundle": "for use admin",
        "doctrine/mongodb-odm-bundle": "To enable support for the Mongodb ODM documents"
    },
    "autoload": {
        "psr-4": {"FDevs\\RoutingBundle\\": ""}
    }
}
